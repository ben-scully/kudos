<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">

      <h1>New Event:</h1>

      <form action="/events" method="post">
        <div class="form-group">
          <label for="eventName">Event Name</label>
          <input type="text" class="form-control" id="eventName" name="eventName" placeholder="Event name">
        </div>
        <div class="form-group">
          <label for="eventDescription">Event Description</label>
          <input type="text" class="form-control" id="eventDescription" name="eventDescription" placeholder="Event description">
        </div>
        <div class="form-group">
          <label for="eventStartdate">Start Date</label>
          <input type="date" class="form-control" id="eventStartdate" name="eventStartdate" >
        </div>
        <div class="form-group">
          <label for="eventEnddate">End Date</label>
          <input type="date" class="form-control" id="eventEnddate" name="eventEnddate" >
        </div>
        <div class="form-group">
          <label for="eventOfficeId">Office</label>
          <input type="text" class="form-control" id="eventOfficeId" name="eventOfficeId" >
        </div>

        <button type="submit" class="btn btn-default">Submit</button>
      </form>

    </div>
  </div>
</div>
