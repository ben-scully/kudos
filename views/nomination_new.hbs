<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <h2>Friday Meeting  -  {{location}}  -  {{dateformat date}}</h2>
    </div>
  </div>

  <div class="row">

    <div class="col-md-12">
      <div class="panel panel-default">

        <div class="panel-heading">
          <p>{{awardcategory}}  -  {{awardcategoryDescription}}</p>
        </div>
        <div class="panel-body">
          <p>...</p>
          <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
          <span class="glyphicon glyphicon-triangle-bottom" aria-hidden="true"></span>
        </div>

        <ul class="list-group">
          <li class="person list-group-item">
            <img height="50px" src="https://img.clipartfest.com/fd7d1ac3605a7cd2954a6ea29a9c88cc_business-person-cartoon-business-person-clipart_326-425.jpeg" alt="">
            <p>{{name}}</p>
            <p>{{nominationDescription}}</p>
            {{#if winner}}
              <span class="glyphicon glyphicon-ok-sign" aria-hidden="true"></span>
            {{else}}
              {{#if (pastDate date)}}
                <span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span>
              {{else}}
                <span class="glyphicon glyphicon-question-sign" aria-hidden="true"></span>
              {{/if}}
            {{/if}}
          </li>
        </ul>

      </div>
    </div>

  </div>
</div>
